package application;

import java.net.URL;
import java.util.ResourceBundle;

import javafx.fxml.FXML;
import javafx.scene.chart.LineChart;
import javafx.scene.chart.XYChart;

public class Customer_Attendance_Line_ChartController implements Initializable {
	
	@FXML
	private LineChart<String,Integer> lineChartAttendance;
	
	@Override
	public void initialize(URL url, ResourceBundle rb){
		XYChart.Series<String, Integer> series = new XYChart.Series<>();
		   
		series.getData().add(new XYChart.Data<>("AM", barchart.calcNumAM()));
		series.getData().add(new XYChart.Data<>("PM", barchart.calcNumPM()));
		
		
		barChartAttendance.getData().addAll(series);   
	}
}
