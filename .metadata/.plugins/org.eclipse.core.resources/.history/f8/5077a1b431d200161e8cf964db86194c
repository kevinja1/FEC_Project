package application;
import java.sql.*;

public class LoginModel {
	
	Connection connection;
	
	public LoginModel(){
		connection = SqliteConnection.Connector();
		if(connection == null){
			System.exit(1);
		}
	}
	
	public boolean isDbConnected(){
		try{
			connection.isClosed();
		} catch (SQL Exception e) {
			e.printStackTrace();
		}
	}
}
